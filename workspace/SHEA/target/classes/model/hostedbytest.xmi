<?xml version="1.0" encoding="ASCII"?>
<featuremodel:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:featuremodel="http://fr.saloon.model.featuremodel"
    xsi:schemaLocation="http://fr.saloon.model.featuremodel ../metamodel/FeatureModel.ecore">
  <constraints
      xsi:type="featuremodel:HostedBy"
      from="//@rootFeature/@subFeatures.0"
      to="//@rootFeature/@nodeFeature.0"/>
  <rootFeature
      name="AppSecurity">
    <subFeatures
        name="FaceRecognition">
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        name="MotionDetection">
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
    </subFeatures>
    <featureCardinality
        cardinalityMin="1"
        cardinalityMax="1"/>
    <nodeFeature
        name="HAB">
      <NodeCardinalities
          cardinalityMin="1"
          cardinalityMax="1"/>
    </nodeFeature>
  </rootFeature>
</featuremodel:FeatureModel>
