<?xml version="1.0" encoding="ASCII"?>
<featuremodel:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:featuremodel="http://fr.saloon.model.featuremodel"
    xmlns:type="http://fr.saloon.model.type"
    xsi:schemaLocation="http://fr.saloon.model.featuremodel ../../../metamodel/FeatureModel.ecore http://fr.saloon.model.type ../../../metamodel/Type.ecore">
  <constraints
      xsi:type="featuremodel:Implies"
      from="//@rootFeature/@subFeatures.0/@variants.0"
      to="//@rootFeature/@subFeatures.2/@variants.2/@variants.2"/>
  <constraints
      xsi:type="featuremodel:Implies"
      from="//@rootFeature/@subFeatures.0/@variants.5"
      to="//@rootFeature/@subFeatures.2/@variants.2/@variants.2"/>
  <constraints
      xsi:type="featuremodel:Implies"
      from="//@rootFeature/@subFeatures.0/@variants.4"
      to="//@rootFeature/@subFeatures.2/@variants.2/@variants.2"/>
  <constraints
      xsi:type="featuremodel:Implies"
      from="//@rootFeature/@subFeatures.0/@variants.1"
      to="//@rootFeature/@subFeatures.2/@variants.2/@variants.1"/>
  <rootFeature
      name="dotCloud">
    <attributes
        name="deploymentModel">
      <value
          xsi:type="type:StringValue"
          value="Public"/>
    </attributes>
    <attributes
        name="serviceModel">
      <value
          xsi:type="type:StringValue"
          value="PaaS"/>
    </attributes>
    <subFeatures
        xsi:type="featuremodel:Alternative"
        name="Language">
      <featureCardinality
          cardinalityMax="1"/>
      <variants
          name="PHP">
        <featureCardinality
            cardinalityMax="5"/>
      </variants>
      <variants
          name="Java">
        <featureCardinality
            cardinalityMax="5"/>
      </variants>
      <variants
          name="Ruby">
        <featureCardinality
            cardinalityMax="5"/>
      </variants>
      <variants
          name="Opa">
        <featureCardinality
            cardinalityMax="5"/>
      </variants>
      <variants
          name="Phyton">
        <featureCardinality
            cardinalityMax="5"/>
      </variants>
      <variants
          name="Perl">
        <featureCardinality
            cardinalityMax="5"/>
      </variants>
    </subFeatures>
    <subFeatures
        name="Framework">
      <subFeatures
          name="Node.js">
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="5"/>
      </subFeatures>
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        xsi:type="featuremodel:Alternative"
        name="Service">
      <featureCardinality
          cardinalityMax="1"/>
      <variants
          name="Search Server">
        <subFeatures
            name="Solr">
          <featureCardinality
              cardinalityMin="1"
              cardinalityMax="5"/>
        </subFeatures>
        <featureCardinality
            cardinalityMax="1"/>
      </variants>
      <variants
          xsi:type="featuremodel:Alternative"
          name="Protocol">
        <featureCardinality
            cardinalityMax="1"/>
        <variants
            name="SMTP">
          <featureCardinality
              cardinalityMax="5"/>
        </variants>
        <variants
            name="SSL">
          <featureCardinality
              cardinalityMax="5"/>
        </variants>
      </variants>
      <variants
          xsi:type="featuremodel:Alternative"
          name="Server">
        <featureCardinality
            cardinalityMax="1"/>
        <variants
            name="Static">
          <featureCardinality
              cardinalityMax="5"/>
        </variants>
        <variants
            name="Jetty">
          <featureCardinality
              cardinalityMax="5"/>
        </variants>
        <variants
            name="Nginx">
          <featureCardinality
              cardinalityMax="5"/>
        </variants>
      </variants>
      <variants
          xsi:type="featuremodel:Alternative"
          name="Database">
        <featureCardinality
            cardinalityMax="1"/>
        <variants
            xsi:type="featuremodel:Alternative"
            name="SQL">
          <featureCardinality
              cardinalityMax="1"/>
          <variants
              name="MySQL">
            <featureCardinality
                cardinalityMax="5"/>
          </variants>
          <variants
              name="PostgreSQL">
            <featureCardinality
                cardinalityMax="5"/>
          </variants>
        </variants>
        <variants
            xsi:type="featuremodel:Alternative"
            name="NoSQL">
          <featureCardinality
              cardinalityMax="1"/>
          <variants
              name="Document Store">
            <subFeatures
                name="MongoDB">
              <featureCardinality
                  cardinalityMax="5"/>
            </subFeatures>
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
          <variants
              name="Key-Value Store">
            <subFeatures
                name="Redis">
              <featureCardinality
                  cardinalityMax="5"/>
            </subFeatures>
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
        </variants>
      </variants>
    </subFeatures>
    <subFeatures
        name="Resource">
      <subFeatures
          name="Memory">
        <attributes
            name="memorySize">
          <value
              xsi:type="type:RangeValue"
              min="32"
              max="2560">
            <value
                value="32"/>
          </value>
        </attributes>
        <attributes
            name="memoryUnitType">
          <value
              xsi:type="type:StringValue"
              value="MB"/>
        </attributes>
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <subFeatures
          name="Disk">
        <attributes
            name="diskSize">
          <value
              xsi:type="type:RangeValue"
              max="20">
            <value/>
          </value>
        </attributes>
        <attributes
            name="diskUnitType">
          <value
              xsi:type="type:StringValue"
              value="GB"/>
        </attributes>
        <featureCardinality
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        name="Location">
      <subFeatures
          name="US East">
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
    </subFeatures>
    <featureCardinality
        cardinalityMin="1"
        cardinalityMax="1"/>
  </rootFeature>
</featuremodel:FeatureModel>
