<?xml version="1.0" encoding="ASCII"?>
<featuremodel:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:featuremodel="http://fr.saloon.model.featuremodel"
    xmlns:type="http://fr.saloon.model.type"
    xsi:schemaLocation="http://fr.saloon.model.featuremodel ../../../metamodel/FeatureModel.ecore http://fr.saloon.model.type ../../../metamodel/Type.ecore">
  <rootFeature
      name="PagodaBox">
    <attributes
        name="deploymentModel">
      <value
          xsi:type="type:StringValue"
          value="Public"/>
    </attributes>
    <attributes
        name="serviceModel">
      <value
          xsi:type="type:StringValue"
          value="Public"/>
    </attributes>
    <subFeatures
        name="Language">
      <subFeatures
          name="PHP">
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        name="Web Process">
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="25"/>
    </subFeatures>
    <subFeatures
        name="Service">
      <subFeatures
          name="Database">
        <subFeatures
            name="Cloud">
          <attributes
              name="dbServiceModel">
            <value
                xsi:type="type:EnumListValue"
                value="//@rootFeature/@subFeatures.2/@subFeatures.0/@subFeatures.0/@attributes.0/@value/@values.1">
              <values
                  id="Private"/>
              <values
                  value="1"
                  id="Public"/>
            </value>
          </attributes>
          <featureCardinality
              cardinalityMin="1"
              cardinalityMax="1"/>
        </subFeatures>
        <subFeatures
            xsi:type="featuremodel:Exclusive"
            name="Kind">
          <featureCardinality
              cardinalityMin="1"
              cardinalityMax="1"/>
          <variants
              name="SQL">
            <subFeatures
                name="MySQL">
              <featureCardinality
                  cardinalityMin="1"
                  cardinalityMax="1"/>
            </subFeatures>
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
          <variants
              name="NoSQL">
            <subFeatures
                name="MongoDB">
              <featureCardinality
                  cardinalityMin="1"
                  cardinalityMax="1"/>
            </subFeatures>
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
        </subFeatures>
        <featureCardinality
            cardinalityMax="25"/>
      </subFeatures>
      <subFeatures
          name="Worker">
        <featureCardinality
            cardinalityMax="25"/>
      </subFeatures>
      <subFeatures
          xsi:type="featuremodel:Alternative"
          name="Caching">
        <featureCardinality
            cardinalityMax="1"/>
        <variants
            name="Redis">
          <featureCardinality
              cardinalityMax="25"/>
        </variants>
        <variants
            name="Memcached">
          <featureCardinality
              cardinalityMax="25"/>
        </variants>
      </subFeatures>
      <subFeatures
          name="Protocol">
        <subFeatures
            xsi:type="featuremodel:Exclusive"
            name="SSL">
          <featureCardinality
              cardinalityMin="1"
              cardinalityMax="1"/>
          <variants
              name="Piggy">
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
          <variants
              name="Self Signed">
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
          <variants
              name="Third Party">
            <featureCardinality
                cardinalityMax="1"/>
          </variants>
        </subFeatures>
        <featureCardinality
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        name="Resource">
      <subFeatures
          name="Memory">
        <attributes
            name="memorySize">
          <value
              xsi:type="type:RangeValue"
              min="200"
              max="385850">
            <value
                value="200"/>
          </value>
        </attributes>
        <attributes
            name="memoryUnitType">
          <value
              xsi:type="type:StringValue"
              value="MB"/>
        </attributes>
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <subFeatures
          name="Disk">
        <attributes
            name="diskSize">
          <value
              xsi:type="type:RangeValue"
              min="10"
              max="-1">
            <value
                value="10"/>
          </value>
        </attributes>
        <attributes
            name="diskUnitType">
          <value
              xsi:type="type:StringValue"
              value="GB"/>
        </attributes>
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <subFeatures
          name="CPU">
        <attributes
            name="cpuCores">
          <value
              xsi:type="type:RangeValue"
              max="25">
            <value/>
          </value>
        </attributes>
        <featureCardinality
            cardinalityMax="1"/>
      </subFeatures>
      <subFeatures
          name="Bandwidth">
        <attributes
            name="bandwidthUnitType">
          <value
              xsi:type="type:StringValue"
              value="GB"/>
        </attributes>
        <attributes
            name="rate">
          <value
              xsi:type="type:RangeValue"
              min="1"
              max="25">
            <value
                value="1"/>
          </value>
        </attributes>
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        name="Location">
      <subFeatures
          name="US">
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <featureCardinality
        cardinalityMin="1"
        cardinalityMax="1"/>
  </rootFeature>
</featuremodel:FeatureModel>
