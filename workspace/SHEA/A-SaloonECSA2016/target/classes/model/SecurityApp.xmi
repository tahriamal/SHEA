<?xml version="1.0" encoding="ASCII"?>
<featuremodel:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:featuremodel="http://fr.saloon.model.featuremodel"
    xmlns:type="http://fr.saloon.model.type"
    xsi:schemaLocation="http://fr.saloon.model.featuremodel ../metamodel/FeatureModel.ecore http://fr.saloon.model.type ../metamodel/Type.ecore">
  <constraints
      xsi:type="featuremodel:Implies"
      from="//@rootFeature/@subFeatures.0/@subFeatures.2/@variants.1"
      to="//@rootFeature/@subFeatures.2/@variants.1"/>
  <rootFeature
      name="SecurityApp">
    <subFeatures
        name="MotionDetection">
      <subFeatures
          name="FlowDecoding">
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
      </subFeatures>
      <subFeatures
          xsi:type="featuremodel:Exclusive"
          name="Recording">
        <featureCardinality
            cardinalityMax="1"/>
        <variants
            name="Live Record">
          <featureCardinality
              cardinalityMax="1"/>
        </variants>
        <variants
            name="Interval-based Record">
          <featureCardinality
              cardinalityMax="1"/>
        </variants>
      </subFeatures>
      <subFeatures
          xsi:type="featuremodel:Alternative"
          name="Flow Capture">
        <featureCardinality
            cardinalityMin="1"
            cardinalityMax="1"/>
        <variants
            name="MPEG">
          <featureCardinality
              cardinalityMax="1"/>
        </variants>
        <variants
            name="SVGGR">
          <featureCardinality
              cardinalityMax="1"/>
        </variants>
      </subFeatures>
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        name="FaceRecognition">
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        xsi:type="featuremodel:Exclusive"
        name="LiveStreaming">
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
      <variants
          name="StandAlone">
        <featureCardinality
            cardinalityMax="1"/>
      </variants>
      <variants
          name="BuiltIn">
        <featureCardinality
            cardinalityMax="1"/>
      </variants>
    </subFeatures>
    <subFeatures
        name="Storage">
      <attributes
          name="DataBase">
        <value
            xsi:type="type:StringValue"
            value="MongoDB"/>
      </attributes>
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
    </subFeatures>
    <featureCardinality
        cardinalityMin="1"
        cardinalityMax="1"/>
  </rootFeature>
</featuremodel:FeatureModel>
