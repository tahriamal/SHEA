    <subFeatures
        name="2">
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <featureCardinality
        cardinalityMin="1"
        cardinalityMax="1"/>
 </rootFeature>
</featuremodel:FeatureModel>