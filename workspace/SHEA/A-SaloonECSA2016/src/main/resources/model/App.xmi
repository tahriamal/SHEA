  <constraints
      xsi:type="featuremodel:Colocated"
      from="//@rootFeature/@subFeatures.0"
      fromAgain="//@rootFeature/@subFeatures.1"/>
  <rootFeature
      name="App">
    <subFeatures
        name="1">
         <featureCardinality
          cardinalityMax="1"/>
