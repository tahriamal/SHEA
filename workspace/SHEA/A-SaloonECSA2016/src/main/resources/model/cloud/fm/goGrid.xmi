<?xml version="1.0" encoding="ASCII"?>
<featuremodel:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:featuremodel="http://fr.saloon.model.featuremodel"
    xmlns:type="http://fr.saloon.model.type"
    xsi:schemaLocation="http://fr.saloon.model.featuremodel ../../../metamodel/FeatureModel.ecore http://fr.saloon.model.type ../../../metamodel/Type.ecore">
  <rootFeature
      name="GoGrid">
    <attributes
        name="deploymentModel">
      <value
          xsi:type="type:StringValue"
          value="Public"/>
    </attributes>
    <attributes
        name="serviceModel">
      <value
          xsi:type="type:StringValue"
          value="PaaS"/>
    </attributes>
    <subFeatures
        name="Cloud Server">
      <attributes
          name="OS">
        <value
            xsi:type="type:EnumListValue"
            value="//@rootFeature/@subFeatures.0/@attributes.0/@value/@values.0">
          <values
              id="linux"/>
          <values
              value="1"
              id="windows"/>
        </value>
      </attributes>
      <attributes
          name="cloudServerDiskSize">
        <value
            xsi:type="type:EnumListValue"
            value="//@rootFeature/@subFeatures.0/@attributes.1/@value/@values.0">
          <values
              value="25"
              id="25"/>
          <values
              value="50"
              id="50"/>
          <values
              value="100"
              id="100"/>
          <values
              value="200"
              id="200"/>
          <values
              value="400"
              id="400"/>
          <values
              value="800"
              id="800"/>
          <values
              value="1200"
              id="1200"/>
        </value>
      </attributes>
      <attributes
          name="cloudServerDiskDataUnitType">
        <value
            xsi:type="type:StringValue"
            value="GB"/>
      </attributes>
      <attributes
          name="cloudServerMemoryDataUnitType">
        <value
            xsi:type="type:StringValue"
            value="MB"/>
      </attributes>
      <attributes
          name="cloudServerMemorySize">
        <value
            xsi:type="type:EnumListValue"
            value="//@rootFeature/@subFeatures.0/@attributes.4/@value/@values.0">
          <values
              value="512"
              id="512"/>
          <values
              value="1024"
              id="1024"/>
          <values
              value="2048"
              id="2048"/>
          <values
              value="4096"
              id="4096"/>
          <values
              value="8192"
              id="8192"/>
          <values
              value="16384"
              id="16384"/>
          <values
              value="24478"
              id="24478"/>
        </value>
      </attributes>
      <attributes
          name="cloudServerCoreNumber">
        <value
            xsi:type="type:EnumListValue"
            value="//@rootFeature/@subFeatures.0/@attributes.5/@value/@values.0">
          <values
              id="0.5"/>
          <values
              value="1"
              id="1"/>
          <values
              value="2"
              id="2"/>
          <values
              value="4"
              id="4"/>
          <values
              value="8"
              id="8"/>
          <values
              value="16"
              id="16"/>
          <values
              value="24"
              id="24"/>
        </value>
      </attributes>
      <attributes
          name="cloudServerSize">
        <value
            xsi:type="type:EnumListValue">
          <values
              id="x-small"/>
          <values
              value="1"
              id="small"/>
          <values
              value="2"
              id="medium"/>
          <values
              value="3"
              id="large"/>
          <values
              value="4"
              id="x-large"/>
          <values
              value="5"
              id="xx-large"/>
          <values
              value="6"
              id="xxx-large"/>
        </value>
      </attributes>
      <featureCardinality
          cardinalityMax="20"/>
    </subFeatures>
    <subFeatures
        name="Service">
      <subFeatures
          name="Load Balancer">
        <featureCardinality
            cardinalityMax="20"/>
      </subFeatures>
      <subFeatures
          name="Cloud Storage">
        <attributes
            name="cloudStorageDiskSize">
          <value
              xsi:type="type:RangeValue"
              max="1024000"/>
        </attributes>
        <featureCardinality
            cardinalityMax="1"/>
      </subFeatures>
      <featureCardinality
          cardinalityMax="1"/>
    </subFeatures>
    <subFeatures
        xsi:type="featuremodel:Exclusive"
        name="Location">
      <featureCardinality
          cardinalityMin="1"
          cardinalityMax="1"/>
      <variants
          name="US-West-1">
        <featureCardinality
            cardinalityMax="1"/>
      </variants>
      <variants
          name="US-East-1">
        <featureCardinality
            cardinalityMax="1"/>
      </variants>
      <variants
          name="EU-West-1">
        <featureCardinality
            cardinalityMax="1"/>
      </variants>
    </subFeatures>
    <featureCardinality
        cardinalityMin="1"
        cardinalityMax="1"/>
  </rootFeature>
</featuremodel:FeatureModel>
